      <a class="paginate-btn num-btn">1</a>
      <a class="paginate-btn num-btn current-btn">2</a>
      <a class="paginate-btn num-btn">3</a></span>
    <a class="paginate-btn next-btn fa fa-caret-right end-view-btn" title="下一页"></a>
  </div>
</div>
<div id="mb"></div>
<div class="conalert"  id="conalert1">
        <img class="conimg" src="assets/images/yimg/yuan001.png">
</div>
<script type="text/javascript">
    $(function(){
        var lastnum = ($(".table .total-num").text()) % 10;
        var delindex = Math.floor(($(".table .total-num").text()) / 10);
        var index =  delindex + 1;
            $(".table .paginate-box").click(function(){
                if($(".table .current-btn").text() == index){ 
                     $(".table .fix tr").slice(lastnum+1).css("display","none")
                }
                else{
                    $(".table .fix tr").css("display","table-row");
                    $(".table .fix table tr td").css("height","52px");
                }
            });

            $(".table .rem").click(function(){
                var reindex = $(this).parent().parent("tr").index();
                $(".table #testtable tr").eq(reindex).remove();
                $(".table .fix tr").eq(reindex).css("display","none");
            });

            $(".table #testtable4 thead .cbr-replaced").click(function(){
                 $(".table #testtable4 tbody .cbr-replaced").toggleClass("cbr-checked")
            })
            //图片鼠标移动事件
            $(".pimg").hover(function(){
            	$(this).find(".icon-input_search").css("display","block");
            },function(){
            	$(this).find(".icon-input_search").css("display","none");
            });
            //图片弹出
            $(".icon-input_search").click(function(){
            	$("#mb,.conalert").css("display","block");
            });
            $("#mb").click(function(){
            	$("#mb,.conalert").css("display","none");
            });
            //如果作废不显示
			if($(".zf").length>0){
				$(".zf").each(function(){
					var zfindex = $(this).parent().parent().index() + 1;
					$("#testtable4 tr").eq(zfindex).find(".lastchild").css("display","none");
				})
			}
			//点击作废
			$(".lastchild").click(function(){
				$(this).css("display","none");
				var zfcindex = $(this).parent().parent().index() - 1;
				$("#testtable tbody tr").eq(zfcindex).find(".state").html('<p class="zf"></p>作废');
			});
			//状态叙述判断
			$("#s2example-3").change(function(){
				if($("#select2-chosen-1").text().match("生效")){
					$(".mesbox").css("display","block").html('<p>?</p>合同未到废止日期');
				}
				if($("#select2-chosen-1").text().match("失效")){
					$(".mesbox").css("display","block").html('<p>?</p>合同超过废止日期');
				}
				if($("#select2-chosen-1").text().match("作废")){
					$(".mesbox").css("display","block").html('<p>?</p>合同非到期原因作废');
				}
			});
			//点击排序效果
			$("#testtable thead tr .top").click(function(){
				$(this).toggleClass("onclickt").siblings("p").removeClass("onclickb");

			});
			$("#testtable thead tr .bottom").click(function(){
				$(this).toggleClass("onclickb").siblings("p").removeClass("onclickt");
			});
			$(".icon-btn_list_downarrow").click(function(){
				var poslist = $(this).offset();
				 $(this).siblings("#show").css("display","block");
				 $(this).siblings("#show").css("top",poslist.top);
				 $(this).siblings("#show").css("left",poslist.left - 40);
					$("#show li").click(function(){
							if($(this).index() == 0){
								$(this).parents("#show").parent("td").find("span").text("渠道");
							}
							else{
								$(this).parents("#show").parent("td").find("span").text("直客");
							}
							$(this).parents("#show").css("display","none");
					})
			})
        })
</script>


</body>
</html>